import geometry;

// FIXME: Hack because imported types do not work yet
struct Point2 {
    x: f32;
    y: f32;
}

struct Rect2 {
    width: f32;
    height: f32;
}


fun main(): i32 {
    let p1: Point2 = geometry.make_point(0.0, 0.0);
    let p2: Point2 = geometry.make_point(3.0, 4.0);
    
    let dist: f32 = geometry.distance_squared(p1, p2); // 25.0
    
    let r: Rect2 = geometry.Rect { width: 5.0, height: 4.0 };
    let a: f32 = geometry.area(r); // 20.0
    
    if (dist > 24.0) {
        if (a > 19.0) {
            if (a < 21.0) {
                return 20;
            }
        }
    }
    
    return 0;
}

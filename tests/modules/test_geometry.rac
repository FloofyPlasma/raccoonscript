// EXPECT: 20
import geometry;

fun main(): i32 {
    let p1 = geometry.make_point(0.0, 0.0);
    let p2 = geometry.make_point(3.0, 4.0);
    
    let dist = geometry.distance_squared(p1, p2); // 25.0
    
    let r = geometry.Rect { width: 5.0, height: 4.0 };
    let a = geometry.area(r); // 20.0
    
    if (dist > 24.0) {
        if (a > 19.0) {
            if (a < 21.0) {
                return 20;
            }
        }
    }
    
    return 0;
}
